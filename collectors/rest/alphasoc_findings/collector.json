{
  "type": "collection",
  "ttl": "4h",
  "removeFields": [],
  "resumeOnBoot": false,
  "schedule": {
    "cronSchedule": "*/5 * * * *",
    "maxConcurrentRuns": 1,
    "skippable": true,
    "run": {
      "rescheduleDroppedTasks": false,
      "maxTaskReschedule": 1,
      "logLevel": "info",
      "jobTimeout": "3m",
      "mode": "run",
      "timeRangeType": "relative",
      "timeWarning": {},
      "expression": "true",
      "minTaskSize": "1MB",
      "maxTaskSize": "10MB",
      "stateTracking": {
        "stateUpdateExpression": "{follow: JSON.parse(_raw).follow}",
        "stateMergeExpression": "newState",
        "enabled": true
      },
      "timestampTimezone": "UTC",
      "earliest": "",
      "latest": ""
    },
    "resumeMissed": false,
    "enabled": true
  },
  "streamtags": [],
  "workerAffinity": false,
  "collector": {
    "conf": {
      "discovery": {
        "discoverType": "none"
      },
      "collectMethod": "get",
      "pagination": {
        "type": "response_body",
        "maxPages": 0,
        "attribute": [
          "more",
          "follow"
        ],
        "lastPageExpr": "more === false"
      },
      "authentication": "basic",
      "timeout": 0,
      "useRoundRobinDns": false,
      "disableTimeFilter": false,
      "decodeUrl": false,
      "rejectUnauthorized": true,
      "captureHeaders": true,
      "safeHeaders": [],
      "retryRules": {
        "type": "backoff",
        "interval": 1000,
        "limit": 5,
        "multiplier": 2,
        "maxIntervalMs": 20000,
        "codes": [
          429,
          503
        ],
        "enableHeader": true,
        "retryConnectTimeout": false,
        "retryConnectReset": false,
        "retryHeaderName": "retry-after"
      },
      "__scheduling": {
        "stateTracking": {}
      },
      "authHeaderKey": "Authorization",
      "authHeaderExpr": "`Bearer ${token}`",
      "collectUrl": "`https://api.alphasoc.net/v1/findings?format=ocsf`",
      "collectRequestHeaders": [
        {
          "name": "accept",
          "value": "'application/json'"
        }
      ],
      "collectRequestParams": [
        {
          "name": "follow",
          "value": "`${follow ? follow : state.follow}`"
        }
      ],
      "username": "'<username_value|Provide your AlphaSOC API key as the username>'",
      "password": "-"
    },
    "destructive": false,
    "encoding": "utf8",
    "type": "rest"
  },
  "input": {
    "type": "collection",
    "staleChannelFlushMs": 10000,
    "sendToRoutes": true,
    "preprocess": {
      "disabled": true
    },
    "throttleRatePerSec": "0",
    "breakerRulesets": [
      "AlphaSOC Large Response Event Breaker"
    ]
  },
  "savedState": {
    "restCollectorState": {
      "data": {
        "follow": ""
      }
    }
  },
  "description": "Documentation: https://docs.alphasoc.com/destinations/cribl/",
  "id": "AlphaSOC_Findings"
}
